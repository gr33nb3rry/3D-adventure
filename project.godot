renderer/rendering_method.mobile="gl_compatibility"
lights_and_shadows/directional_shadow/soft_shadow_filter_quality=4
lights_and_shadows/positional_shadow/soft_shadow_filter_quality.mobile=3
global_illumination/gi/use_half_resolution=true
shading/overrides/force_vertex_shading=true
environment/ssao/quality=0
environment/ssao/adaptive_target=0.0
environment/ssil/quality=0
environment/ssil/adaptive_target=0.0
global_illumination/sdfgi/probe_ray_count=2
environment/volumetric_fog/volume_size=32
anti_aliasing/quality/msaa_3d=2
anti_aliasing/quality/use_debanding=true
lights_and_shadows/positional_shadow/atlas_size=512
2d/sdf/scale=0

